#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,c,d;
	    scanf("%d %d %d %d",&a,&b,&c,&d);
	    int res=a-c;
	    int ser=b-d;
	    if(res>ser){
	        printf("Second\n");
	    }
	    else if(ser>res){
	        printf("First\n");
	    }
	    else{
	        printf("Any\n");
	    }
	}

}

